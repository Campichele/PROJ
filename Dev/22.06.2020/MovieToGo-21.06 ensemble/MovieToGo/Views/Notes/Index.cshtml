@model IEnumerable<MovieToGo.Models.Note>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Note1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdFilmNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUserNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Note1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdFilmNavigation.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUserNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNote">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdNote">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNote">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
